<?xml version="1.0" encoding="utf-8" ?>
<Page
    x:Class="FluentFin.Views.JellyfinSettings.GeneralSettingsPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:FluentFin.Views.JellyfinSettings"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <local:ServerConfigurationPage
        Items="{x:Bind ViewModel.Items, Mode=OneWay}"
        ResetCommand="{x:Bind ViewModel.ResetCommand}"
        SaveCommand="{x:Bind ViewModel.SaveCommand}"
        SectionName="General" />

</Page>
